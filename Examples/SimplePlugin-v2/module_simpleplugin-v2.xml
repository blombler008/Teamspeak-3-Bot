<?xml version="1.0" encoding="UTF-8"?>
<project name="module_simpleplugin-v2" default="compile.module.simpleplugin-v2">
  <dirname property="module.simpleplugin-v2.basedir" file="${ant.file.module_simpleplugin-v2}"/>
  
  <property name="module.jdk.home.simpleplugin-v2" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.simpleplugin-v2" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.simpleplugin-v2" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.simpleplugin-v2" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="simpleplugin-v2.output.dir" value="${module.simpleplugin-v2.basedir}/../../out/production/SimplePlugin-v2"/>
  <property name="simpleplugin-v2.testoutput.dir" value="${module.simpleplugin-v2.basedir}/../../out/test/SimplePlugin-v2"/>
  
  <path id="simpleplugin-v2.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="simpleplugin-v2.module.production.classpath">
    <path refid="${module.jdk.classpath.simpleplugin-v2}"/>
    <pathelement location="${teamspeak_bot.output.dir}"/>
    <path refid="library.teamspeak3-api-1.2.0.classpath"/>
  </path>
  
  <path id="simpleplugin-v2.runtime.production.module.classpath">
    <pathelement location="${simpleplugin-v2.output.dir}"/>
    <path refid="teamspeak_bot.runtime.production.module.classpath"/>
    <path refid="library.teamspeak3-api-1.2.0.classpath"/>
  </path>
  
  <path id="simpleplugin-v2.module.classpath">
    <path refid="${module.jdk.classpath.simpleplugin-v2}"/>
    <pathelement location="${simpleplugin-v2.output.dir}"/>
    <pathelement location="${teamspeak_bot.testoutput.dir}"/>
    <pathelement location="${teamspeak_bot.output.dir}"/>
    <path refid="library.teamspeak3-api-1.2.0.classpath"/>
  </path>
  
  <path id="simpleplugin-v2.runtime.module.classpath">
    <pathelement location="${simpleplugin-v2.testoutput.dir}"/>
    <pathelement location="${simpleplugin-v2.output.dir}"/>
    <path refid="teamspeak_bot.runtime.module.classpath"/>
    <path refid="library.teamspeak3-api-1.2.0.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.simpleplugin-v2">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.simpleplugin-v2">
    <patternset refid="excluded.from.module.simpleplugin-v2"/>
  </patternset>
  
  <path id="simpleplugin-v2.module.sourcepath">
    <dirset dir="${module.simpleplugin-v2.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.simpleplugin-v2" depends="compile.module.simpleplugin-v2.production,compile.module.simpleplugin-v2.tests" description="Compile module SimplePlugin-v2"/>
  
  <target name="compile.module.simpleplugin-v2.production" depends="register.custom.compilers,compile.module.teamspeak_bot" description="Compile module SimplePlugin-v2; production classes">
    <mkdir dir="${simpleplugin-v2.output.dir}"/>
    <javac2 destdir="${simpleplugin-v2.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.simpleplugin-v2}/javac">
      <compilerarg line="${compiler.args.simpleplugin-v2}"/>
      <bootclasspath refid="simpleplugin-v2.module.bootclasspath"/>
      <classpath refid="simpleplugin-v2.module.production.classpath"/>
      <src refid="simpleplugin-v2.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.simpleplugin-v2"/>
    </javac2>
    
    <copy todir="${simpleplugin-v2.output.dir}">
      <fileset dir="${module.simpleplugin-v2.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.simpleplugin-v2.tests" depends="register.custom.compilers,compile.module.simpleplugin-v2.production" description="compile module SimplePlugin-v2; test classes" unless="skip.tests"/>
  
  <target name="clean.module.simpleplugin-v2" description="cleanup module">
    <delete dir="${simpleplugin-v2.output.dir}"/>
    <delete dir="${simpleplugin-v2.testoutput.dir}"/>
  </target>
</project>